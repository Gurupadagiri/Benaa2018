                                @*<input type="text" name="forumhandle" class="inputClass" />*@
                                @Html.TextBoxFor(a => a.usermasters.UserContactInfo, new { @class = "inputClass" })
                            </div>
                            <div class="col-md-12 col-sm-12 col-xs-12">
                                <label>Default Time Clock Labor Code:</label>
                                @*<input type="text" name="forumhandle" class="inputClass" />*@
                                @Html.TextBoxFor(a => a.usermasters.UserDefaultTimeClock, new { @class = "inputClass" })
