                                @Html.TextBoxFor(a => a.usermasters.UserPhone, "this is default value", new { maxlength = 50, @class = "inputClass" })
                            </div>
                            <div class="col-md-6 col-sm-6 col-xs-12">
                                @Html.LabelFor(a => a.usermasters.UserCellEmail)
                                @*<input type="text" name="cell" class="inputClass" />*@
                                @Html.TextBoxFor(a => a.usermasters.UserCellEmail, "this is default value", new { maxlength = 50, @class = "inputClass" })
                            </div>
