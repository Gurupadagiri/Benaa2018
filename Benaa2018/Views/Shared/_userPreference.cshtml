                                </div>
                            </div>
                            <div class="col-md-6 col-sm-6 col-xs-12">
                                @Html.LabelFor(a => a.usermasters.UserName)
                                @Html.TextBoxFor(a => a.usermasters.UserName, "this is default value", new { maxlength = 50, @class = "inputClass" })
                            </div>
                            <div class="col-md-6 col-sm-6 col-xs-12 passwordHolder">
