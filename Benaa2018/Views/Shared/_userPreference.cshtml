@model Benaa2018.Helper.ViewModels.UserPreferenceViewModel
<form id="frmSubmitUserPreferenceMaster" enctype="multipart/form-data" method="post">
    <div class="modal-dialog modal-lg modalWidth1200">
        <div class="panel panel-primary customPopup">
            <div class="panel-heading themeColor">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
                <h4 class="panel-title" id="contactLabel">Internal User Details</h4>
            </div>
            <div id="errorDiv" style="color:red;display:none">
                @*<input style="color:red" type="text" disabled="disabled" id="txtErrorMessage" />*@
                <span style="color:red" id="spnErrorMessgae"></span>
            </div>

            <div class="panel-body">

                <div class="userPreferenceHolder">
                    <div class="col-md-12 saveButtonHolder">
                        @*<input type="submit" name="" value="Save" id="" class="blueButton button-save">*@
                        <input type="submit" name="" value="Save" id="saveUserPreference" class="blueButton button-save">
                    </div>
                    <div class="logInHolder">
                        <div class="logInLeft col-md-6 col-sm-6 col-xs-12">
                            <div class="col-md-6 col-sm-6 col-xs-12">
                                @Html.LabelFor(a => a.usermasters.UserFirstName)
                                @Html.TextBoxFor(a => a.usermasters.UserFirstName, "this is default value", new { maxlength = 50, @class = "inputClass" })
                            </div>
                            <div class="col-md-6 col-sm-6 col-xs-12">
                                @Html.LabelFor(a => a.usermasters.UserLastName)
                                @Html.TextBoxFor(a => a.usermasters.UserLastName, "this is default value", new { maxlength = 50, @class = "inputClass" })
                            </div>
                            <div class="col-md-12 col-sm-12 col-xs-12">
                                @Html.LabelFor(a => a.usermasters.UserEmail)
                                @Html.TextBoxFor(a => a.usermasters.UserEmail, "this is default value", new { maxlength = 50, @class = "form-control input-lg" })

