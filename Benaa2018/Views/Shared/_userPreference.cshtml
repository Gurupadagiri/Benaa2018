                                    @*<span class="contactLabel"><input type="checkbox" name="" checked="checked" onclick="AdminDetailPage.OnChangeChkUserEnable(this);"><label for="">User Enabled</label></span>*@
                                    <span class="contactLabel">
                                        @Html.CheckBoxFor(a => a.usermasters.UserEnabled) @Html.LabelFor(a => a.usermasters.UserEnabled)
