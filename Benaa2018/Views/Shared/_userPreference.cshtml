                                </div>
                                @Html.LabelFor(a => a.usermasters.UserPassword)
                                @Html.PasswordFor(a => a.usermasters.UserPassword, new { maxlength = 50, @class = "inputClass" })
                                @*<input type="password" name="flasttname" class="inputClass" />*@
                                @*<input type="submit" value="Reset" class="ResetButton" />*@
                                <button type="button" class="ResetButton">Login</button>

                            </div>
                            <div class="col-md-12 col-sm-12 col-xs-12">
                                <label>Forum Handle</label>
                                @*<input type="text" name="forumhandle" class="inputClass" />*@
