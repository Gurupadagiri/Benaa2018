        <div class="col-md-3 pull-right">
            <input id="chkSetUp" type="checkbox" name="" data-off-text="Off" data-on-text="On" asp-for="chkStatus">
            <span class="chkText">Show Inactive Codes</span>
        </div>
</div>
        </div>
    </form>
<script src="~/js/common.js"></script>
