                                                <label>Tags</label>
                                                <select class="form-control required" data-val="true" data-val-required="The ProjectColorId field is required.">
                                                    <option value="" selected="selected">Unassigned</option>
                                                </select>
                                                <div class="editHolder">
                                                    <a href="javascript:void(0);" id="" title="Add a Tag">
                                                        <img id="" border="0" src="images/add.gif" style="border-width:0px;">
                                                    </a>
                                                    <a href="javascript:void(0);" title="Edit / Delete a Tag">
                                                        <img id="" border="0" src="images/edit.gif" style="border-width:0px;">
                                                    </a>
                                                </div>
